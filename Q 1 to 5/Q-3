 //WAP to below pattern        A B C D E        A B C D        A B C         A  B        A4.
 #include<stdio.h>
 void main()
 {
    int i,j,n=5;
    for(i=1;i<=n;i++)
    {
        for(j=1;j<=n-i+1;j++)
        printf(" ");
        for(j=1;j<=i;j++)
        printf("%c ",j+64);
        

    }
 }
 
    
 