#include<stdio.h>

void main() {
    int no = 734;
    int reversedNumber = 0; 
    int remainder; 

    
    while (no != 0) {
        remainder = no % 10; 
        reversedNumber = reversedNumber * 10 + remainder; 
        no = no / 10; 
    }

    
    printf("Reversed Number: %f\n", reversedNumber);
}